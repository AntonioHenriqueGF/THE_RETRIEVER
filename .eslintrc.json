{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["node_modules/**", "www/**"],
    "rules": {
        "comma-dangle": ["error", "always-multiline"],
        "no-undef": "off",
        "semi": ["error", "always"],
        "no-unused-vars": "warn",
        "space-before-function-paren": ["error", "never"],
    }
}
